{
  "name": "libram",
  "version": "0.7.15",
  "description": "JavaScript helper library for KoLmafia",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": "https://github.com/loathers/libram",
  "author": "Samuel Gaus <sam@gaus.co.uk>",
  "license": "MIT",
  "private": false,
  "scripts": {
    "build": "yarn run build:tsc && yarn run build:bundled",
    "build:tsc": "tsc",
    "build:bundled": "node build.mjs",
    "clean": "rm -rf dist",
    "docs": "yarn run typedoc",
    "format": "yarn run prettier --write .",
    "lint": "yarn run eslint src tools --ext .ts && yarn run prettier --check .",
    "prepublishOnly": "yarn run build",
    "updateProps": "yarn run ts-node ./tools/parseDefaultProperties.ts"
  },
  "files": [
    "dist/**/*.js",
    "dist/**/*.d.ts"
  ],
  "devDependencies": {
    "@babel/compat-data": "^7.17.0",
    "@babel/core": "^7.17.2",
    "@babel/plugin-proposal-object-rest-spread": "^7.16.7",
    "@babel/plugin-transform-runtime": "^7.15.0",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-typescript": "^7.15.0",
    "@tsconfig/node16": "^1.0.2",
    "@types/jest": "^27.0.1",
    "@types/lodash-es": "^4.17.4",
    "@types/node": "^16.11.11",
    "@types/node-fetch": "^2.5.7",
    "@typescript-eslint/eslint-plugin": "^5.5.0",
    "@typescript-eslint/parser": "^5.5.0",
    "esbuild": "^0.17.0",
    "esbuild-plugin-babel": "^0.2.3",
    "eslint": "^7.16.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jest": "^25.2.3",
    "eslint-plugin-jsdoc": "^40.0.1",
    "eslint-plugin-libram": "^0.2.29",
    "husky": "^4.3.6",
    "java-parser": "^1.4.0",
    "jest": "^27.1.0",
    "kolmafia": "^5.27260.0",
    "lint-staged": ">=10",
    "node-fetch": "^2.6.1",
    "prettier": "^2.1.2",
    "ts-jest": "^27.0.5",
    "ts-node": "^10.4.0",
    "typedoc": "^0.22.10",
    "typescript": "^4.5.2"
  },
  "peerDependencies": {
    "kolmafia": "^5.26781.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{ts,js}": "prettier --write"
  },
  "sideEffects": false
}
